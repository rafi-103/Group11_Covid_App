<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.project_covid_app.Ayman.PrescriptionController"
            prefWidth="750" prefHeight="400">

    <TableView fx:id="tblPrescriptions"
               layoutX="10" layoutY="10"
               prefWidth="730" prefHeight="330">
        <columns>
            <TableColumn text="ID" fx:id="colRxID"/>
            <TableColumn text="Patient" fx:id="colPatientID"/>
            <TableColumn text="Med" fx:id="colMedication"/>
            <TableColumn text="Date" fx:id="colDate"/>
        </columns>
    </TableView>
    <Button text="Verify" layoutX="340" layoutY="350" onAction="#onVerifyPrescription"/>
</AnchorPane>
